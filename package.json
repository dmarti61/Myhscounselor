{
  "name": "my-hs-counselor",
  "version": "1.0.0",
  "homepage": "/",
  "private": true,
  "description": "Personalized career guidance platform for high school students choosing between college and trade paths.",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postbuild": "react-snap"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "^5.0.1",
    "react-router-dom": "^6.30.1",
    "jspdf": "^3.0.1"
  },
  "devDependencies": {
    "react-snap": "^1.23.0",
    "@netlify/plugin-sitemap": "^0.8.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "puppeteerArgs": ["--no-sandbox", "--disable-setuid-sandbox"],
    "include": [
      "/",
      "/not-sure",
      "/explore-careers",
      "/quiz",
      "/preferences",
      "/results",
      "/college-guide",
      "/two-year",
      "/military",
      "/direct-entry-careers",
      "/trade",
      "/talk-templates",
      "/financial-aid",
      "/decision-official",
      "/job-search",
      "/first-months",
      "/gap-year",
      "/about",
      "/contact"
    ],
    "source": "build",
    "destination": "build"
  }
}
